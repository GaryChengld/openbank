{
  "base_path": "/apis/internal/v2/consentapp",
  "logo_uri": "/apis/internal/v2/consentapp/images/logo.png",
  "consentPath": "{{ edgeBasePath }}/apis/internal/v2/consentapp/consent",
  "cookieName":"OB_SESSION",
  "sessionImpl":"./cookie_session_impl",
  "cookieKey" : "{{ consentSessionKey }}",
  "loginJWTKey": "{{ jwtSignKey }}",
  "styles": {
    "header_background_color": "#FFF"
  },
  "loginApplication": {
    "transactionEndpoint": "{{ edgeBasePath }}/apis/internal/v2/loginapp"
  },
  "scaApplication": {
    "transactionEndpoint": "/apis/internal/v2/consentapp/otp"
  },
  "authenticationTransaction": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/connector/v1/authenticate",
    "method": "POST",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "customerTransaction": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/connector/v2/customers",
    "method": "GET",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "accessTokenTransaction": {
    "transactionEndpoint": "{{ edgeBasePath }}/apis/v2/oauth/authorized",
    "method": "POST",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "generateOtp": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/apis/v1/sms/otp/",
    "method": "POST",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "validateOtp": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/apis/v1/sms/otp/",
    "method": "POST",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "getAccounts": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/connector/v2/ais/accounts",
    "method": "GET",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "getAccountRequest": {
    "transactionEndpoint": "{{ edgeBasePath }}/internal/connector/v2/ais/account-requests/",
    "method": "GET",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "getPaymentRequest": {
    "transactionEndpoint": "{{ edgeBasePath }}/apis/v2/payments-connector/payments/",
    "method": "GET",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "createConsent": {
    "transactionEndpoint": "{{ edgeBasePath }}/apis/internal/v2/consent/",
    "method": "POST",
    "headers": {
      "x-apikey": "{{internalAppKey}}"
    }
  },
  "errors": {
    "customerNotFound": "NOT_FOUND_CUSTOMER",
    "accountNotFound": "NOT_FOUND_ACCOUNT",
    "accountRequestNotFound": "NOT_FOUND_ACCOUNT_REQUEST",
    "paymentRequestNotFound": "NOT_FOUND_PAYMENT_REQUEST",
    "invalidSession": "INVALID_SESSION",
    "invalidType": "INVALID_SCOPE",
    "consentNotCreated": "UNABLE_TO_CREATE_CONSENT",
    "userCancelConsent": "CONSENT_REJECTED",
    "internalError": "INTERNAL_ERROR_OCCURED",
    "invalidOtp": "INVALID_OTP"
  }
}



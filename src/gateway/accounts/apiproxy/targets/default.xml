<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request/>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request>
            <Step>
                <Name>Jsc.SetConnectorURL</Name>
            </Step>
            <Step>
                <Name>Assign-Account-List-in-headers</Name>
            </Step>
        </Request>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>{{ edgeBasePath }}/internal/connector/v2/accounts{account_number}{pathSuffix}</URL>
    </HTTPTargetConnection>
</TargetEndpoint>
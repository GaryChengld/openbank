<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-App-Attributes">
    <DisplayName>Extract App Attributes</DisplayName>
    <Properties/>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="Fault.InvalidAccessToken">
    <DisplayName>Fault.InvalidAccessToken</DisplayName>
    <FaultRules/>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json" variablePrefix="@" variableSuffix="#">
    {
        "status": 401,
        "message": "Invalid Access Token",
        "description": "The Access token used is either invalid or has expired",
        "code": 10000,
        "more_info": "http://www.example.com/codes/10000"
    }
            </Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
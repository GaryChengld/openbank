<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Add-JWS-Header">
    <DisplayName>Add JWS Header</DisplayName>
    <Properties/>
    <Add>
        <Headers>
            <Header name="x-jws-signature">{detachedJws}</Header>
        </Headers>
    </Add>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
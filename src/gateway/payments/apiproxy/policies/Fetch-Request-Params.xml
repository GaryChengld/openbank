<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Fetch-Request-Params">
    <DisplayName>Fetch Request Params</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>ipdempotentKey</Name>
        <Ref>request.header.X-Idempotency-Key</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>PaymentRequestId</Name>
        <Ref>accesstoken.request_id</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>TokenType</Name>
        <Ref>accesstoken.type</Ref>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
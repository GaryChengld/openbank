<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign-Message-Replace-result">
    <DisplayName>Assign: Response</DisplayName>
    <Properties/>
 	<Set>
        <Headers>
                <Header name="Access-Control-Allow-Origin">{request.header.origin}</Header>
                <Header name="Access-Control-Allow-Credentials">true</Header>
                <Header name="Access-Control-Allow-Methods">OPTIONS,GET,POST,PUT,DELETE,HEAD</Header>
                <Header name="Access-Control-Allow-Headers">Authorization,Content-Type,Accept,Origin,X-Requested-With,X-CSDKVersion</Header>
                <Header name="Access-Control-Max-Age">86400</Header>
            </Headers>
        <Payload contentType="application/json" variablePrefix="%" variableSuffix="#">{
"transfer":{
	"status":"INITIATED",
    "transaction_id":"%uuid#"
  }
}
      </Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="resposne"/>
</AssignMessage>

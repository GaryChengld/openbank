  "basePath": "/apis/v1/opendata/locations",
  "paths": {
    "/atms": {
      "get": {
        "description": "<p>Returns a list of ATMs with Address, Latitude and Longitude.</p><p>&nbsp;</p><p>Optional parameters include search filters, latitude, longitide, zipcode and radius of search.</p>",
        "summary": "List of atms",
        "operationId": "atms",        
        "tags": [
          "opendata"
        ],
        "produces": [
          "application/json; charset=utf-8"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/AtmsResponse"
            }
          },
          "400": {
            "description": "Bad Request"
          }
        },
        "security": [],
        "parameters": [
          {
            "in": "query",
            "name": "lat",
            "description": "Latitude of the place around which you want to find ATMs. If latitude is provided, longitude must also be provided.",
            "type": "string",
            "required": false
          },
          {
            "in": "query",
            "name": "long",
            "description": "Lontitude of the place around which you want to find ATMs. If lontitude is provided, latitude must also be provided.",
            "type": "string",
            "required": false
          },
          {
            "in": "query",
            "name": "zipcode",
            "description": "Zipcode of the place around which you want to find ATMs.",
            "type": "string",
            "required": false
          },
          {
            "in": "query",
            "name": "distance_unit",
            "description": "Indicates the unit for the radius, as well as the units of the distance of each location from the basepoint in the response.",
            "type": "string",
            "required": false,
            "enum": [
              "mile",
              "kilometer"
            ]
          },
          {
            "in": "query",
            "name": "radius",
            "description": "This is the radius from the search point in the distance unit you set.",
            "type": "string",
            "required": false
          }
          ]
      }
    },
